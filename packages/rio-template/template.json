{
  "dependencies": {
    "@rio-cloud/iframe-resizer": "0.0.5",
    "@sentry/browser": "^5.9.1",
    "classlist-polyfill": "^1.2.0",
    "connected-react-router": "^6.6.0",
    "custom-event-polyfill": "^1.0.7",
    "history": "^4.10.1",
    "less": "^3.10.3",
    "lodash": "^4.17.15",
    "react-app-polyfill": "^1.0.6",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "unorm": "^1.6.0"
  },
  "devDependencies": {
    "@testing-library/react": "^9.3.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.0.0",
    "core-js": "^3.6.4",
    "css-loader": "^3.2.0",
    "eslint-plugin-compat": "^3.3.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "jest-changed-files": "^24.9.0",
    "jest-environment-jsdom": "^24.9.0",
    "less-loader": "^5.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^9.4.3",
    "node-env-run": "^3.0.2",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.1",
    "react": "16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "16.12.0",
    "react-intl": "^3.12.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-explorer": "^2.1.1",
    "style-loader": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": ["prettier --write", "git add"]
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run-script dev-mock-server\" \"react-app-rewired start --scripts-version @rio-cloud/react-scripts\"",
    "build": "react-app-rewired --max_old_space_size=4096 build --scripts-version @rio-cloud/react-scripts",
    "test": "react-app-rewired test --scripts-version @rio-cloud/react-scripts",
    "pretest": "npm run download-externals",
    "eject": "react-scripts eject",
    "check-licenses": "bash ./check_licenses.sh",
    "ci-install": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true npm install",
    "audit": "npm audit --registry https://registry.npmjs.org --parseable",
    "dev-mock-server": "nodenv mocks/rio-starter-devmocks.js --exec nodemon",
    "download-externals": "nodenv ./loadExternals.js",
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\"",
    "format-code": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "jest": {
    "moduleNameMapper": {
      "ReactDOM": "react-dom"
    },
    "moduleDirectories": [
      "node_modules",
      "mocks"
    ]
  }
}
